          0,
          1,
          2,
          3
        ],
        "type": "integer",
        "format": "int32"
      },
      "CreateAppointmentDto": {
        "required": [
          "customerId",
          "employeeId",
          "endTime",
          "serviceId",
          "startTime"
        ],
        "type": "object",
        "properties": {
          "startTime": {
            "type": "string",
            "format": "date-time"
          },
          "endTime": {
            "type": "string",
            "format": "date-time"
          },
          "serviceId": {
            "type": "string",
            "format": "uuid"
          },
          "employeeId": {
            "minLength": 1,
            "type": "string"
          },
          "customerId": {
            "minLength": 1,
            "type": "string"
          },
          "notes": {
            "maxLength": 300,
            "minLength": 0,
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CreateBusinessDto": {
        "required": [
          "name"
        ],
        "type": "object",
        "properties": {
          "name": {
            "maxLength": 150,
            "minLength": 0,
            "type": "string"
          },
          "address": {
            "maxLength": 150,
            "minLength": 0,
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CreateServiceDto": {
        "required": [
          "businessId",
          "description",
          "name"
        ],
        "type": "object",
        "properties": {
          "name": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "description": {
            "maxLength": 500,
            "minLength": 1,
            "type": "string"
          },
          "price": {
            "minimum": 0,
            "type": "number",
            "format": "double"
          },
          "durationInMinutes": {
            "maximum": 2147483647,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "businessId": {
            "type": "string",
            "format": "uuid"
          }
        },
        "additionalProperties": false
      },
      "UpdateAppointmentDto": {
        "type": "object",
